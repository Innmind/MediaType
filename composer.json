{
    "name": "innmind/media-type",
    "type": "library",
    "description": "Model of media types",
    "keywords": ["media type"],
    "homepage": "http://github.com/Innmind/MediaType",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/MediaType/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/immutable": "~4.15|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\MediaType\\": "src/",
            "Fixtures\\Innmind\\MediaType\\": "fixtures/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\MediaType\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~10.2",
        "innmind/black-box": "~5.0",
        "vimeo/psalm": "~5.6",
        "innmind/coding-standard": "~2.0"
    },
    "conflict": {
        "innmind/black-box": "<5.0|~6.0"
    },
    "suggest": {
        "innmind/black-box": "For property based testing"
    },
    "provide": {
        "innmind/black-box-sets": "5.0"
    }
}
